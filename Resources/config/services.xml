<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="html2canvasproxybundle.services.html2canvasproxy.class">HTML2Canvas\ProxyBundle\Controller\Html2CanvasProxyController</parameter>
    </parameters>

    <services>
        <service id="html2canvasproxybundle.services.html2canvasproxy" class="%html2canvasproxybundle.services.html2canvasproxy.class%">
            <argument type="service" id="request_stack"/>
            <argument></argument>
            <argument type="service" id="frontbundle.container.current_landing_site_container"/>
        </service>
    </services>
</container>
